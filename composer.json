{
    "name": "wave-design/craft-hr-commencement-import-utility",
    "description": "Convert XLSX to HTML",
    "type": "craft-plugin",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^3.0.0",
        "phpoffice/phpspreadsheet": "*"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "wavedesign\\crafthrcommencementimportutility\\": "src/"
        }
    },
    "extra": {
        "handle": "_hr-commencement-import-utility",
        "name": "HR Commencement Import Utility",
        "developer": "WAVEDesign",
        "documentationUrl": "",
        "class": "wavedesign\\crafthrcommencementimportutility\\ImportUtility"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

