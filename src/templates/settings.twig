{% import "_includes/forms" as forms %}
<input type="hidden" name="action" value="plugins/save-plugin-settings">

{{ forms.textField({
  first: true,
  label: "folderId",
  name: 'folderId',
  value: settings.folderId
}) }}

{{ forms.textField({
  first: true,
  label: "fieldId",
  name: 'fieldId',
  value: settings.fieldId
}) }}

{{ forms.textField({
  first: true,
  label: "filePath",
  name: 'filePath',
  value: settings.filePath
}) }}

